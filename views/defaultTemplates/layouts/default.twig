<html>
<head>
    {% block head %}
        {{ woodlet.wp_head() }}
        <title>{% block title %}{{ woodlet.the_title() }}{% endblock %}</title>
        {% block baseTag %}
            <base href="{{ woodlet.get_home_url() }}" />
        {% endblock %}
        {% block viewport %}
            <meta name="viewport" content="width=device-width, user-scalable=yes">
        {% endblock %}
    {% endblock %}
</head>
<body {{ woodlete.body_class() }}>
{% block body %}
    {% block view %}{% endblock %}
    {{ woodlet.wp_footer() }}
{% endblock %}
</body>
</html>
